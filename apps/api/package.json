{
  "name": "@akashnetwork/console-api",
  "version": "3.27.0",
  "description": "Api providing data to the deploy tool",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/akash-network/console"
  },
  "license": "Apache-2.0",
  "author": "Akash Network",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:app && npm run build:akash-templates",
    "build:akash-templates": "tsup scripts/buildAkashTemplatesCache.ts -d dist/scripts && node dist/scripts/buildAkashTemplatesCache.js",
    "build:app": "NODE_ENV=production tsup",
    "build:container": "dc build api",
    "console": "npm run build:app && node dist/console.js",
    "dev": "npm run start",
    "dev-nodc": "npm run start",
    "format": "prettier --write ./*.{js,json} **/*.{ts,js,json}",
    "lint": "eslint .",
    "migration:gen": "drizzle-kit generate",
    "prod": "doppler run -- node dist/server.js",
    "release": "release-it",
    "start": "tsup --watch",
    "start:dev": "tsup src/server.ts --watch",
    "test": "jest --selectProjects unit functional",
    "test:ci-setup": "dc up -d db mock-oauth2-server provider-proxy",
    "test:ci-teardown": "dc down",
    "test:cov": "jest --selectProjects unit functional --coverage --reporters=default --reporters=jest-junit",
    "test:e2e": "jest --selectProjects e2e",
    "test:functional": "jest --selectProjects functional",
    "test:functional:cov": "npm run test:functional -- --coverage",
    "test:functional:watch": "npm run test:functional -- --watch",
    "test:oauth2-server": "dc up:dev -d mock-oauth2-server",
    "test:unit": "jest --selectProjects unit",
    "test:unit:cov": "jest --selectProjects unit --coverage",
    "test:unit:watch": "jest --selectProjects unit --watch",
    "test:watch": "jest --selectProjects unit functional --watch"
  },
