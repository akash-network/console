{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["SKIP_DC_DB"],
  "tasks": {
    "transit": {
      "dependsOn": ["^transit"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["transit"]
    },
    "test": {
      "dependsOn": ["transit"]
    },
    "dev": {
      "persistent": true,
      "cache": false,
      "dependsOn": ["//#dc:up:db"],
      "env": ["SKIP_DB"]
    },
    "dev-nodc": {
      "persistent": true,
      "cache": false
    },
    "//#dc:up:db": {},
    "update-apps-local-deps": {
      "outputs": ["mvm.lock"]
    }
  },
  "ui": "tui"
}
